; http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[**.js]
indent_style = space
indent_size = 2

[**.ts]
indent_style = space
indent_size = 2

[**.json]
indent_style = space
indent_size = 2

[**.vue]
indent_style = space
indent_size = 2